<div class="tab-pane fade show active"id="rabbit"role="tabpanel">
    <div class="tab-single">
        <div class="row">
            {% for rabbit in rabbits  %}
            <!--Start-->
            <div class="col-xl-3 col-lg-4 col-md-4 col-12">
                <div class="single-product">
                    <div class="product-img">
                        <a href="#">
                            <img src="{{ rabbit.photoFile }}" style="height: 170px;width: 320px" class="main-img" alt="#">
                        </a>
                        <div class="button-head">
                            <div class="action-button">
                                <a data-toggle="modal" data-target="#exampleModal"
                                   title="Quick view" href="{{ path('petsList') }}">
                                    <i class="far fa-eye"></i><span>View List</span>
                                </a>
                                <a title="Wishlist" href="{{ path('pet_cart_add',{'id':rabbit.id}) }}"><i class="far fa-heart"></i>
                                    <span>Add to Wishlist</span></a>
                                <a title="shopping"href="#"><i class="fas fa-shopping-bag"></i>
                                    <span>Quick Shop</span></a>

                            </div>
                            <div class="button-low">
                                <a title="Add to cart" href="{{ path('contact') }}">Contact Now</a>
                            </div>
                        </div>
                    </div>
                    <div class="product-title">
                        <h3>{{ rabbit.name }}</h3>
                        <div class="product-price">
                            <p>A {{ rabbit.gender }} {{ rabbit.color }} rabbit </p>
                            <span>{{ rabbit.age }} age</span>
                        </div>
                    </div>
                </div>
            </div>
{% endfor %}